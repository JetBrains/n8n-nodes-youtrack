{
	"name": "@jetbrains/n8n-nodes-youtrack",
	"version": "0.1.7",
	"description": "Custom n8n node for YouTrack",
	"license": "MIT",
	"homepage": "https://github.com/JetBrains/n8n-nodes-youtrack",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "JetBrains",
		"email": "youtrack-n8n-publishers@jetbrains.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JetBrains/n8n-nodes-youtrack.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"test": "vitest run"
	},
	"publishConfig": {
		"access": "public"
	},
	"npm": {
		"publish": false
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/PermanentTokenCredentialForYouTrackApi.credentials.js",
			"dist/credentials/YouTrackWebhookAuthApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/YouTrack/Youtrack.node.js",
			"dist/nodes/YouTrack/YoutrackTrigger.node.js"
		]
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@n8n/eslint-plugin-community-nodes": "^0.7.0",
		"@n8n/node-cli": "^0.17.0",
		"eslint": "9.32.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.4",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2",
		"typescript-eslint": "^8.48.1",
		"vitest": "^4.0.14"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
